<% searchBar=false; sidebarTitle="mm" %>
    <%- include('./../templates/sd-head.ejs') %>
        <!DOCTYPE html>
        <html lang="en">

        <body>
            <%- include('./../templates/sd-header.ejs') %>
                <%- include('./../templates/sd-sidebar.ejs') %>

                    <main id="main" class="main">
                        <div class="pagetitle">
                            <h1>
                                <%= title%>
                            </h1>
                        </div>
                        <!-- End Page Title -->

                        <section class="section dashboard">
                            <div class="row">
                                <!-- Left side columns -->
                                <div class="col-lg-12">
                                    <div class="row">
                                        <%- include('./sd-dateChange.ejs') %>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="row">

                                        <%- include('./../templates/sd-headCards.ejs') %>
                                    </div>
                                </div>
                                <!-- End Left side columns -->
                            </div>
                            <div class="row">

                                <div class="col-lg-6">
                                    <% chartTitle="Sales Funnel of " +title %>
                                        <%- include('./../templates/sd-funnel.ejs') %>
                                </div>
                                <div class="col-lg-6">
                                    <% chartTitle="Sales target achievement of " +title %>
                                        <%- include('./../templates/sd-circleChartSingle.ejs') %>
                                </div>
                                <div class="col-lg-6">
                                    <% chartTitle="Comparision of " +title +" with other marketing managers"%>
                                        <%- include('./../templates/sd-radar.ejs') %>
                                </div>
                                <div class="col-lg-6">
                                    <% chartTitle="Leads Vs Sales of " +title %>
                                        <%- include('./../templates/sd-graph.ejs') %>
                                </div>
                                <div class="col-lg-6">
                                    <% chartTitle="Sales converted for DMCs " %>
                                        <%- include('./../templates/sd-barApex.ejs') %>
                                </div>
                                <div class="col-lg-6">
                                    <% chartTitle="Revenue share per DMC " %>
                                        <%- include('./../templates/sd-pie.ejs') %>
                                </div>
                                <div class="col-lg-12">
                                    <div class="row">
                                        <%-include('./../templates/sd-recentSalesTable.ejs') %>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </main>
                    <!-- End #main -->

                    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i
                            class="bi bi-arrow-up-short"></i></a>
                    <%- include('./../templates/sd-footer.ejs') %>
        </body>
        <%- include('./../templates/sd-footerScript.ejs') %>

        </html>